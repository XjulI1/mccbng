{
  "name": "back_loopback",
  "version": "1.0.0",
  "description": "API mCcBng",
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "build": "lb-tsc",
    "build:watch": "lb-tsc --watch",
    "lint": "yarn run eslint && yarn run prettier:check",
    "lint:fix": "yarn run eslint:fix && yarn run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "yarn run prettier:cli -l",
    "prettier:fix": "yarn run prettier:cli --write",
    "eslint": "lb-eslint --report-unused-disable-directives .",
    "eslint:fix": "yarn run eslint --fix",
    "pretest": "yarn run rebuild",
    "test": "lb-mocha --allow-console-logs \"dist/__tests__\"",
    "posttest": "yarn run lint",
    "test:dev": "lb-mocha --allow-console-logs dist/__tests__/**/*.js && yarn run posttest",
    "premigrate": "yarn run build",
    "migrate": "node ./dist/migrate",
    "preopenapi-spec": "yarn run build",
    "openapi-spec": "node ./dist/openapi-spec",
    "rebuild": "yarn run clean && yarn run build",
    "prestart": "yarn run rebuild",
    "start": "node -r source-map-support/register .",
    "clean": "lb-clean dist *.tsbuildinfo .eslintcache",
    "docker:staging:build": "docker build -t dockregistry.xju.fr/mccbng/api:staging .",
    "docker:staging:push": "docker push dockregistry.xju.fr/mccbng/api:staging",
    "docker:latest:build": "docker tag dockregistry.xju.fr/mccbng/api:staging dockregistry.xju.fr/mccbng/api:latest",
    "docker:latest:push": "docker push dockregistry.xju.fr/mccbng/api:latest",
    "docker:run": "docker run -p 3000:3000 --name mccbng_api --rm --mount type=bind,source=/Users/xjulien/Git/Perso/mccbng/back_loopback/dist/datasources/mccb-mysql.datasource.config.json,target=/home/node/app/dist/datasources/mccb-mysql.datasource.config.json -d dockregistry.xju.fr/mccbng/api:staging",
    "docker:inspect": "docker exec -it mccbng_api bash",
    "docker:stop": "docker stop mccbng_api"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "Xavier JULIEN <xavijulien@gmail.com>",
  "license": "MIT",
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@loopback/authentication": "^7.3.0",
    "@loopback/authentication-jwt": "^0.9.0",
    "@loopback/boot": "^3.4.0",
    "@loopback/core": "^2.16.0",
    "@loopback/repository": "^3.6.0",
    "@loopback/rest": "^9.3.0",
    "@loopback/rest-explorer": "^3.3.0",
    "@loopback/service-proxy": "^3.2.0",
    "compression": "^1.0.3",
    "cors": "^2.5.2",
    "express": "^4.17.1",
    "helmet": "^4.6.0",
    "loopback": "^3.0.0",
    "loopback-boot": "^2.6.5",
    "loopback-connector-mysql": "^2.4.1",
    "pm2": "^4.2.3",
    "request-promise": "^4.2.2",
    "sequelize": "^3.30.2",
    "serve-favicon": "^2.0.1",
    "tslib": "^2.0.0"
  },
  "devDependencies": {
    "@loopback/build": "^6.4.0",
    "@loopback/eslint-config": "^10.2.0",
    "@loopback/testlab": "^3.4.0",
    "@types/express": "^4.17.9",
    "@types/node": "^10.17.59",
    "eslint": "^7.25.0",
    "eslint-config-loopback": "^13.1.0",
    "nodemon": "^2.0.2",
    "nsp": "^3.2.1",
    "source-map-support": "^0.5.19",
    "typescript": "~4.2.4"
  }
}
