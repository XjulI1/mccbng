{
  "name": "back_loopback_4",
  "version": "0.0.1",
  "description": "API Loopback 4",
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=10.16"
  },
  "scripts": {
    "build": "lb-tsc",
    "build:watch": "lb-tsc --watch",
    "lint": "yarn run eslint && yarn run prettier:check",
    "lint:fix": "yarn run eslint:fix && yarn run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "yarn run prettier:cli -l",
    "prettier:fix": "yarn run prettier:cli --write",
    "eslint": "lb-eslint --report-unused-disable-directives .",
    "eslint:fix": "yarn run eslint --fix",
    "pretest": "yarn run rebuild",
    "test": "lb-mocha --allow-console-logs \"dist/__tests__\"",
    "posttest": "yarn run lint",
    "test:dev": "lb-mocha --allow-console-logs dist/__tests__/**/*.js && yarn run posttest",
    "docker:build": "docker build -t back_loopback_4 .",
    "docker:run": "docker run -p 3000:3000 -d back_loopback_4",
    "premigrate": "yarn run build",
    "migrate": "node ./dist/migrate",
    "preopenapi-spec": "yarn run build",
    "openapi-spec": "node ./dist/openapi-spec",
    "rebuild": "yarn run clean && yarn run build",
    "prestart": "yarn run rebuild",
    "start": "node -r source-map-support/register .",
    "clean": "lb-clean dist *.tsbuildinfo .eslintcache"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "Xavier JULIEN <xjulien@molotov.tv>",
  "license": "MIT",
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@loopback/boot": "^3.1.2",
    "@loopback/booter-lb3app": "^2.2.15",
    "@loopback/core": "^2.13.1",
    "@loopback/repository": "^3.3.0",
    "@loopback/rest": "^9.1.1",
    "@loopback/rest-explorer": "^3.0.5",
    "@loopback/service-proxy": "^3.0.5",
    "compression": "^1.0.3",
    "cors": "^2.5.2",
    "express": "^4.17.1",
    "helmet": "^3.21.2",
    "loopback": "^3.0.0",
    "loopback-boot": "^2.6.5",
    "loopback-component-explorer": "^5.0.0",
    "loopback-connector-mysql": "^2.4.1",
    "pm2": "^4.2.3",
    "request-promise": "^4.2.2",
    "sequelize": "^3.30.2",
    "serve-favicon": "^2.0.1",
    "tslib": "^2.0.0"
  },
  "devDependencies": {
    "@loopback/build": "^6.2.8",
    "@loopback/eslint-config": "^10.0.4",
    "@loopback/testlab": "^3.2.10",
    "@types/express": "^4.17.9",
    "@types/node": "^10.17.48",
    "db-migrate": "^0.11.11",
    "db-migrate-mysql": "^2.1.1",
    "eslint": "^7.15.0",
    "eslint-config-loopback": "^13.1.0",
    "nodemon": "^2.0.2",
    "nsp": "^3.2.1",
    "source-map-support": "^0.5.19",
    "typescript": "~4.1.2"
  }
}
